  "SettingDefinitions": [
    {
      "Type": "textbox",
      "Value": {
        "Key": "btc_addresses",
        "Label": "i18n:btc_addresses",
        "Tooltip": "i18n:btc_addresses_tooltip",
        "DefaultValue": "",
        "MaxLines": 5,
        "Style": {
          "LabelWidth": 120,
          "Width": 450
        }
      }
    },
    {
      "Type": "newline"
    },
    {
      "Type": "textbox",
      "Value": {
        "Key": "eth_addresses",
        "Label": "i18n:eth_addresses",
        "Tooltip": "i18n:eth_addresses_tooltip",
        "DefaultValue": "",
        "MaxLines": 5,
        "Style": {
          "LabelWidth": 120,
          "Width": 450
        }
      }
    },
    {
      "Type": "newline"
    },
    {
      "Type": "select",
      "Value": {
        "Key": "currency",
        "Label": "i18n:currency",
        "Tooltip": "i18n:currency_tooltip",
        "DefaultValue": "USD",
        "Options": [
          {
            "Label": "USD",
            "Value": "USD"
          },
          {
            "Label": "CNY",
            "Value": "CNY"
          },
          {
            "Label": "EUR",
            "Value": "EUR"
          },
          {
            "Label": "JPY",
            "Value": "JPY"
          }
        ],
        "Style": {
          "LabelWidth": 120,
          "Width": 450
        }
      }
    },
    {
      "Type": "newline"
    },
    {
      "Type": "textbox",
      "Value": {
        "Key": "alchemy_api_key",
        "Label": "i18n:alchemy_api_key",
        "Tooltip": "i18n:alchemy_api_key_tooltip",
        "DefaultValue": "",
        "Style": {
          "LabelWidth": 120,
          "Width": 450
        }
      }
    },
    {
      "Type": "newline"
    },
    {
      "Type": "textbox",
      "Value": {
        "Key": "min_value",
        "Label": "i18n:min_value",
        "Tooltip": "i18n:min_value_tooltip",
        "DefaultValue": "0",
        "Style": {
          "LabelWidth": 120,
          "Width": 450
        }
      }
    }
  ]
